          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "integer"
                  },
                  "nome": {
                    "type": "string"
                  },
                  "sobrenome": {
                    "type": "string"
                  },
                  "matricula": {
                    "type": "string"
                  },
                  "telefones": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                },
                "required": [
                  "nome",
                  "sobrenome",
                  "matricula",
                  ""
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Estudante atualizado com sucesso"
          }
        }
      },
      "patch": {
        "summary": "Atualiza parcialmente um estudante",
        "operationId": "patchStudent",
        "parameters": [
